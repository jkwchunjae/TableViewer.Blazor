
@if (Type == typeof(string) && Data is string str)
{
    <MudTextField T="string" Label="@Name" Value="str" Placeholder="@Name"/>
}
else if (Type == typeof(int) && Data is int intnum)
{
    <MudTextField T="int" Label="@Name" Value="intnum"/>
}
else
{
    <TeObjectEditor Type="Type" Data="Data" Name="Name"/>
}