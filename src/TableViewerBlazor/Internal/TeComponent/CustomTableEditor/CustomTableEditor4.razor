@inherits CustomTableEditor3<T, TParent1, TItem1, TParent2, TItem2, TParent3, TItem3>
@typeparam T
@typeparam TParent1
@typeparam TItem1
@typeparam TParent2
@typeparam TItem2
@typeparam TParent3
@typeparam TItem3
@typeparam TParent4
@typeparam TItem4

<MudForm @ref="form"
    Class="d-block"
    IsValid="@success"
    IsValidChanged="OnValidChanged"
    Errors="@errors"
    ErrorsChanged="OnErrorsChanged"
>
    <TeElementEditor
        Data="Data"
        DataChanged="@(async value => await OnDataChanged(value))"
        Options="Options"
        Depth="1"
        Path="@string.Empty"
    />
</MudForm>
