@inherits CustomTableEditor1<T, TParent1, TItem1>
@typeparam T
@typeparam TParent1
@typeparam TItem1
@typeparam TParent2
@typeparam TItem2

<MudForm @ref="form"
    Class="d-block"
    IsValid="@success"
    IsValidChanged="OnValidChanged"
    Errors="@errors"
    ErrorsChanged="OnErrorsChanged"
>
    <TeElementEditor
        Data="Data"
        DataChanged="@(async value => await OnDataChanged(value))"
        Options="Options"
        Depth="1"
        Path="@string.Empty"
    />
</MudForm>
