@inherits TeEditorBase

<MudSelect
    T="object"
    Value="Data"
    ValueChanged="@(async value => await OnValueChanged(value))"
    Label="@Label"

    OuterClass="@(SelectBoxOption.Property?.OuterClass ?? default)"
    InputClass="@(SelectBoxOption.Property?.InputClass ?? default)"
    PopoverClass="@(SelectBoxOption.Property?.PopoverClass ?? default)"
    ListClass="@(SelectBoxOption.Property?.ListClass ?? default)"
    Dense="@(SelectBoxOption.Property?.Dense ?? default)"
    OpenIcon="@(SelectBoxOption.Property?.OpenIcon ?? default)"
    CloseIcon="@(SelectBoxOption.Property?.CloseIcon ?? default)"
    SelectAll="@(SelectBoxOption.Property?.SelectAll ?? default)"
    SelectAllText="@(SelectBoxOption.Property?.SelectAllText ?? default)"
    Delimiter="@(SelectBoxOption.Property?.Delimiter ?? default)"
    SelectedValues="@(SelectBoxOption.Property?.SelectedValues ?? default)"
    Comparer="@(SelectBoxOption.Property?.Comparer ?? default)"
    MultiSelection="@(SelectBoxOption.Property?.MultiSelection ?? default)"
    MaxHeight="@(SelectBoxOption.Property?.MaxHeight ?? 300)"
    AnchorOrigin="@(SelectBoxOption.Property?.AnchorOrigin ?? default)"
    TransformOrigin="@(SelectBoxOption.Property?.TransformOrigin ?? default)"
    Strict="@(SelectBoxOption.Property?.Strict ?? default)"
    Clearable="@(SelectBoxOption.Property?.Clearable ?? default)"
    LockScroll="@(SelectBoxOption.Property?.LockScroll ?? default)"
    CheckedIcon="@(SelectBoxOption.Property?.CheckedIcon ?? default)"
    UncheckedIcon="@(SelectBoxOption.Property?.UncheckedIcon ?? default)"
    IndeterminateIcon="@(SelectBoxOption.Property?.IndeterminateIcon ?? default)"
    Disabled="@(SelectBoxOption.Property?.Disabled ?? default)"
    ReadOnly="@(SelectBoxOption.Property?.ReadOnly ?? default)"
    FullWidth="@(SelectBoxOption.Property?.FullWidth ?? default)"
    Immediate="@(SelectBoxOption.Property?.Immediate ?? default)"
    DisableUnderLine="@(!(SelectBoxOption.Property?.Underline ?? default))"
    HelperText="@(SelectBoxOption.Property?.HelperText ?? default)"
    HelperTextOnFocus="@(SelectBoxOption.Property?.HelperTextOnFocus ?? default)"
    AdornmentIcon="@(SelectBoxOption.Property?.AdornmentIcon ?? default)"
    AdornmentText="@(SelectBoxOption.Property?.AdornmentText ?? default)"
    Adornment="@(SelectBoxOption.Property?.Adornment ?? default)"
    OnlyValidateIfDirty="@(SelectBoxOption.Property?.OnlyValidateIfDirty ?? default)"
    AdornmentColor="@(SelectBoxOption.Property?.AdornmentColor ?? default)"
    AdornmentAriaLabel="@(SelectBoxOption.Property?.AdornmentAriaLabel ?? default)"
    IconSize="@(SelectBoxOption.Property?.IconSize ?? default)"
    Variant="@(SelectBoxOption.Property?.Variant ?? default)"
    Margin="@(SelectBoxOption.Property?.Margin ?? default)"
    Placeholder="@(SelectBoxOption.Property?.Placeholder ?? default)"
    Counter="@(SelectBoxOption.Property?.Counter ?? default)"
    MaxLength="@(SelectBoxOption.Property?.MaxLength ?? default)"
    AutoFocus="@(SelectBoxOption.Property?.AutoFocus ?? default)"
    Lines="@(SelectBoxOption.Property?.Lines ?? default)"
    Text="@(SelectBoxOption.Property?.Text ?? default)"
    TextUpdateSuppression="@(SelectBoxOption.Property?.TextUpdateSuppression ?? default)"
    InputMode="@(SelectBoxOption.Property?.InputMode ?? default)"
    Pattern="@(SelectBoxOption.Property?.Pattern ?? default)"
    ShrinkLabel="@(SelectBoxOption.Property?.ShrinkLabel ?? default)"
    Format="@(SelectBoxOption.Property?.Format ?? default)"
    Required="@(SelectBoxOption.Property?.Required ?? default)"
    Culture="@(SelectBoxOption.Property?.Culture ?? default)"
>
    @foreach (var item in SelectBoxOption.Items)
    {
        <MudSelectItem Value="@item.Value">@item.Text</MudSelectItem>
    }
</MudSelect>

    @* ToStringFunc="@Converter1" *@
