@inherits MudButton

<MudButton
    OnClick="OnClickPopupButton"
    ButtonType="ButtonType"
    Href="@Href"
    Target="@Target"
    DropShadow="DropShadow"
    Ripple="Ripple"
    StartIcon="@StartIcon"
    EndIcon="@EndIcon"
    IconColor="IconColor"
    IconClass="@IconClass"
    Color="Color"
    Size="Size"
    Variant="Variant"
    FullWidth="FullWidth"
    Class="@Class"
    Style="@Style"
    Tag="Tag"
    UserAttributes="UserAttributes"
>
    @PopupAction.Label
</MudButton>