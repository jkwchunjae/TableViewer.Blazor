@typeparam T

<MudForm @ref="form"
    IsValid="@success"
    IsValidChanged="OnValidChanged"
    Errors="@errors"
    ErrorsChanged="OnErrorsChanged"
>
    <TeElementEditor
        Data="Data"
        DataChanged="@(async value => await OnDataChanged(value))"
        Options="Options"
        Depth="1"
        Path="@string.Empty"
    />
</MudForm>
