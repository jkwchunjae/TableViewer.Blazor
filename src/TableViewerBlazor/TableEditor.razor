@typeparam T

<MudForm @ref="form"
    IsValid="@success"
    IsValidChanged="OnValidChanged"
    @bind-Errors="@errors"
>
    <TeElementEditor Type="@typeof(T)"
        Data="Data"
        DataChanged="@(async value => await OnDataChanged(value))"
        Options="Options"
        Depth="1"
    />
</MudForm>
